    "src/**/*.{js,jsx,json,css}": [
      "eslint --fix",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "export NODE_ENV=development && webpack-dev-server -p --hot --progress --profile --colors --open",
